<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://java.sun.com/xml/ns/javaee" xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" version="2.5">
  <welcome-file-list>
    <welcome-file>index.jsp</welcome-file>
  </welcome-file-list>
  <servlet>
    <servlet-name>LogInPage</servlet-name>
    <jsp-file>/login.jsp</jsp-file>
  </servlet>
  <servlet-mapping>
    <servlet-name>LogInPage</servlet-name>
    <url-pattern>/LogInPage</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>LogInGetCookie</servlet-name>
    <servlet-class>servlets.LogInGetCookie</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>LogInGetCookie</servlet-name>
    <url-pattern>/LogInGetCookie</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>SendCoinAction</servlet-name>
    <servlet-class>servlets.SendCoin</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>SendCoinAction</servlet-name>
    <url-pattern>/SendCoinAction</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>Home</servlet-name>
    <jsp-file>/index.jsp</jsp-file>
  </servlet>
