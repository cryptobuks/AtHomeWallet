    <servlet-name>Home</servlet-name>
    <url-pattern>/Home</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>Addresses</servlet-name>
    <jsp-file>/addresses.jsp</jsp-file>
  </servlet>
  <servlet-mapping>
    <servlet-name>Addresses</servlet-name>
    <url-pattern>/Addresses</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>SendCoin</servlet-name>
    <jsp-file>/sendcoin.jsp</jsp-file>
  </servlet>
  <servlet-mapping>
    <servlet-name>SendCoin</servlet-name>
    <url-pattern>/SendCoin</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>Settings</servlet-name>
    <jsp-file>/settings.jsp</jsp-file>
  </servlet>
  <servlet-mapping>
    <servlet-name>Settings</servlet-name>
    <url-pattern>/Settings</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>Transactions</servlet-name>
    <jsp-file>/transactions.jsp</jsp-file>
  </servlet>
  <servlet-mapping>
    <servlet-name>Transactions</servlet-name>
    <url-pattern>/Transactions</url-pattern>
  </servlet-mapping>
</web-app>